pipeline {
	agent any
	
	tools{
		maven 'maven 3.9.1'
		jdk 'jdk17'
	
	}
	
	stages {
		stage('checkout'){
			steps {
				git 'https://github.com/vcc25git/CarApp.git'
			}
		}
		stage('Build'){
			steps {
				sh 'mvn clean compile'
			}
		}
		stage('test'){
			steps {
			 	sh "mvn test"
			 	junit '**/target/surefire-reports/TEST-*.xml'
			 }
		}
		stage('Package') {
			steps {
				sh "mvn package"
			}
		}		
	}
}